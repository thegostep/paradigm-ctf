    "@openzeppelin/contracts": "^3.4.0-solc-0.7",
    "@types/chai": "^4.2.14",
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.14.25",
    "@uniswap/v2-core": "^1.0.1",
    "@uniswap/v2-periphery": "^1.1.0-beta.0",
    "chai": "^4.3.0",
    "ethereum-waffle": "^3.2.2",
    "ethers": "^5.0.28",
    "hardhat": "^2.0.8",
    "hardhat-gas-reporter": "^1.0.4",
    "prettier": "^2.2.1",
